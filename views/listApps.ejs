<body> 

<div class="navbar">
  <div class="navbar-inner">
    <a class="brand" href="#">List Apps</a>
    <ul class="nav">
	  <li ><a href="/logout">Logout</a></li>
  </div>
</div>
 
<p>
  
  
<table class="table table-bordered" width="70%" align="center">
<% if (list.length) { %>
  <% list.forEach(function(element){
	if (dbname+'.system.indexes' != element.name) {
		%>
		<tr>
			<td align="center">
				<a href="/logs/<%= element.name.substring(dbname.length+1) %>"><%= element.name.substring(dbname.length+1) %></a>
			</td> 
		</tr>
  <%}  }) %>
<% } %>
</table>



  
